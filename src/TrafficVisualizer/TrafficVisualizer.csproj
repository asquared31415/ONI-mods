<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup>
        <Configurations>Vanilla;Mergedown</Configurations>
        <RootNamespace>TrafficVisualizer</RootNamespace>
        <AssemblyName>TrafficVisualizer</AssemblyName>
        <Platforms>AnyCPU</Platforms>
    </PropertyGroup>
    <PropertyGroup Condition="'$(Configuration)' == 'Vanilla'" />
    <PropertyGroup Condition="'$(Configuration)' == 'Mergedown'" />
    <PropertyGroup>
        <UseSquareLib>true</UseSquareLib>
    </PropertyGroup>
    <ItemGroup>
        <CopyFolder Include="assets/**">
            <DestName>assets</DestName>
        </CopyFolder>
    </ItemGroup>
    <ItemGroup>
      <Folder Include="archived_versions\" />
    </ItemGroup>
</Project>